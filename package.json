{
  "name": "config-expander-cli",
  "version": "0.0.0-semantic-release",
  "publishConfig": {
    "access": "public"
  },
  "main": "src/config-expander-cli.mjs",
  "description": "cli to work with config-expander managed configs",
  "keywords": [
    "cli"
  ],
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "bin": {
    "config-expander": "src/config-expander-cli.mjs"
  },
  "scripts": {
    "test": "ava --timeout 2m",
    "cover": "c8 -x 'tests/**/*' --temp-directory build/tmp ava --timeout 2m && c8 report -r lcov -o build/coverage --temp-directory build/tmp",
    "docs": "documentation readme src/config-expander-cli.mjs --section=API",
    "lint": "documentation lint src/config-expander-cli.mjs"
  },
  "dependencies": {
    "commander": "^4.0.1",
    "config-expander": "^11.0.3"
  },
  "devDependencies": {
    "ava": "^3.5.2",
    "c8": "^7.1.0",
    "documentation": "^12.2.0",
    "execa": "^3.3.0",
    "semantic-release": "^17.0.4"
  },
  "engines": {
    "node": ">=12.16.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arlac77/config-expander-cli.git"
  },
  "bugs": {
    "url": "https://github.com/arlac77/config-expander-cli/issues"
  },
  "homepage": "https://github.com/arlac77/config-expander-cli#readme",
  "template": {
    "inheritFrom": [
      "arlac77/template-esm-only",
      "arlac77/template-arlac77-github"
    ]
  }
}
