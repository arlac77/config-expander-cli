{
  "name": "config-expander-cli",
  "version": "0.0.0-semantic-release",
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/index.js",
  "module": "src/config-expander.js",
  "description": "cli to work with config-expander managed configs",
  "keywords": [
    "cli"
  ],
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "bin": {
    "config-expander": "bin/config-expander"
  },
  "scripts": {
    "cover": "c8 -x 'tests/**/*' --temp-directory build/tmp ava && c8 report -r lcov -o build/coverage --temp-directory build/tmp",
    "docs": "documentation readme src/config-expander.js --section=API",
    "lint": "documentation lint src/config-expander.js",
    "posttest": "npm run prepare && markdown-doctest",
    "prepare": "rollup -c",
    "pretest": "rollup -c",
    "test": "ava"
  },
  "dependencies": {
    "commander": "^4.0.1",
    "config-expander": "^11.0.3"
  },
  "devDependencies": {
    "@rollup/plugin-json": "^4.0.0",
    "ava": "^2.4.0",
    "builtin-modules": "^3.1.0",
    "c8": "^6.0.1",
    "documentation": "^12.1.4",
    "esm": "^3.2.25",
    "execa": "^3.3.0",
    "markdown-doctest": "^1.0.0",
    "rollup": "^1.27.6",
    "rollup-plugin-cleanup": "^3.1.1",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-executable": "^1.5.1",
    "rollup-plugin-node-resolve": "^5.2.0",
    "semantic-release": "^15.13.31"
  },
  "engines": {
    "node": ">=12.13.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arlac77/config-expander-cli.git"
  },
  "bugs": {
    "url": "https://github.com/arlac77/config-expander-cli/issues"
  },
  "homepage": "https://github.com/arlac77/config-expander-cli#readme",
  "ava": {
    "files": [
      "tests/*-test.mjs"
    ],
    "require": [
      "esm"
    ],
    "babel": false,
    "compileEnhancements": false,
    "extensions": [
      "mjs"
    ]
  },
  "template": {
    "repository": {
      "url": "https://github.com/arlac77/npm-package-template.git"
    }
  }
}
