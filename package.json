{
  "name": "config-expander-cli",
  "version": "0.0.0-semantic-release",
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": "./src/config-expander-cli.mjs"
  },
  "description": "cli to work with config-expander managed configs",
  "keywords": [
    "cli"
  ],
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "bin": {
    "config-expander": "src/config-expander-cli.mjs"
  },
  "scripts": {
    "test": "npm run test:ava",
    "test:ava": "ava --timeout 2m tests/*-ava.mjs tests/*-ava-node.mjs && ava --timeout 4m tests/*-ava.mjs tests/*-ava-node.mjs",
    "cover": "c8 -x 'tests/**/*' --temp-directory build/tmp ava --timeout 4m tests/*-ava.mjs tests/*-ava-node.mjs && c8 report -r lcov -o build/coverage --temp-directory build/tmp",
    "docs": "documentation readme --section=API ./src/**/*.mjs",
    "lint": "npm run lint:docs",
    "lint:docs": "documentation lint ./src/**/*.mjs"
  },
  "dependencies": {
    "commander": "^11.0.0",
    "config-expander": "^13.0.8"
  },
  "devDependencies": {
    "ava": "^5.3.1",
    "c8": "^8.0.1",
    "documentation": "^14.0.2",
    "execa": "^8.0.1",
    "semantic-release": "^22.0.5"
  },
  "engines": {
    "node": ">=14.19.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arlac77/config-expander-cli"
  },
  "bugs": {
    "url": "https://github.com/arlac77/config-expander-cli/issues"
  },
  "homepage": "https://github.com/arlac77/config-expander-cli#readme",
  "template": {
    "inheritFrom": [
      "arlac77/template-arlac77-github",
      "arlac77/template-esm-only"
    ]
  }
}
