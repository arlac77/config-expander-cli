{
  "name": "config-expander-cli",
  "version": "0.0.0-semantic-release",
  "publishConfig": {
    "access": "public"
  },
  "main": "src/config-expander.mjs",
  "module": "src/config-expander.mjs",
  "description": "cli to work with config-expander managed configs",
  "keywords": [
    "cli"
  ],
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "bin": {
    "config-expander": "bin/config-expander"
  },
  "scripts": {
    "prepare": "rollup -c",
    "pretest": "rollup -c",
    "test": "ava --timeout 2m",
    "cover": "c8 -x 'tests/**/*' --temp-directory build/tmp ava --timeout 2m && c8 report -r lcov -o build/coverage --temp-directory build/tmp",
    "docs": "documentation readme src/config-expander.mjs --section=API",
    "lint": "documentation lint src/config-expander.mjs"
  },
  "dependencies": {
    "commander": "^5.0.0",
    "config-expander": "^11.0.3"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^11.1.0",
    "@rollup/plugin-json": "^4.0.3",
    "@rollup/plugin-node-resolve": "^7.1.3",
    "ava": "^3.7.0",
    "builtin-modules": "^3.1.0",
    "c8": "^7.1.0",
    "documentation": "^12.3.0",
    "execa": "^4.0.0",
    "rollup": "^2.6.1",
    "rollup-plugin-cleanup": "^3.1.1",
    "rollup-plugin-executable": "^1.5.3",
    "semantic-release": "^17.0.6"
  },
  "engines": {
    "node": ">=12.16.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arlac77/config-expander-cli.git"
  },
  "bugs": {
    "url": "https://github.com/arlac77/config-expander-cli/issues"
  },
  "homepage": "https://github.com/arlac77/config-expander-cli#readme",
  "template": {
    "inheritFrom": [
      "arlac77/template-cli-app",
      "arlac77/template-github"
    ]
  }
}
