{
  "name": "config-expander-cli",
  "version": "0.0.0-semantic-release",
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/index.js",
  "module": "src/config-expander.js",
  "description": "cli to work with config-expander managed configs",
  "keywords": [
    "cli"
  ],
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "bin": {
    "config-expander": "bin/config-expander"
  },
  "scripts": {
    "cover": "c8 --temp-directory build/coverage ava && c8 report -r lcov --temp-directory build/coverage",
    "test": "ava",
    "posttest": "npm run prepare && markdown-doctest",
    "pretest": "rollup -c",
    "prepare": "rollup -c",
    "docs": "documentation readme src/config-expander.js --section=API",
    "lint": "documentation lint src/config-expander.js",
    "semantic-release": "semantic-release",
    "travis-deploy-once": "travis-deploy-once"
  },
  "dependencies": {
    "caporal": "^1.1.0",
    "config-expander": "^9.1.8"
  },
  "devDependencies": {
    "ava": "^1.0.1",
    "documentation": "^9.1.1",
    "execa": "^1.0.0",
    "markdown-doctest": "^0.9.1",
    "rollup": "^0.68.2",
    "rollup-plugin-cleanup": "^3.0.0",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-executable": "^1.3.0",
    "rollup-plugin-json": "^3.1.0",
    "rollup-plugin-node-resolve": "^4.0.0",
    "semantic-release": "^15.13.1",
    "travis-deploy-once": "^5.0.11",
    "c8": "^3.2.1",
    "esm": "^3.0.84"
  },
  "engines": {
    "node": ">=10.14.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arlac77/config-expander-cli.git"
  },
  "bugs": {
    "url": "https://github.com/arlac77/config-expander-cli/issues"
  },
  "homepage": "https://github.com/arlac77/config-expander-cli#readme",
  "ava": {
    "files": [
      "tests/*-test.js",
      "tests/*-test.mjs"
    ],
    "require": [
      "esm"
    ],
    "babel": false,
    "compileEnhancements": false,
    "extensions": [
      "js",
      "mjs"
    ]
  },
  "template": {
    "repository": {
      "url": "https://github.com/arlac77/npm-package-template.git"
    }
  }
}
